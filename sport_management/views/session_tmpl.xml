<odoo>
    <data>

        <!-- sport.session_tmpl calendar view -->
        <record id="model_name_view_calendar" model="ir.ui.view">
            <field name="name">sport.session_tmpl.view.calendar</field>
            <field name="model">sport.session_tmpl</field>
            <field name="arch" type="xml">
                <calendar string="Calendar Description" date_start="start_date" color="room">
                    <field name="activity_id"/>
                    <field name="coach_id"/>
                    <field name="room"/>
                </calendar>
            </field>
        </record>

    </data>
</odoo>